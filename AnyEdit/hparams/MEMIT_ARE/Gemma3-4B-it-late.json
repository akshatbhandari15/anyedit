{
    "model_name": "Gemma3-4B-IT",
    "layers": [22],
    "clamp_norm_factor": 2.0,
    "layer_selection": "all",
    "fact_token": "subject_last",
    "v_num_grad_steps": 6,
    "v_lr": 0.05,
    "v_loss_layer": 32,
    "v_weight_decay": 1e-3,
    "kl_factor": 1.0,
    "mom2_adjustment": true,
    "mom2_update_weight": 30000,
    "rewrite_module_tmp": "model.language_model.layers.{}.mlp.down_proj",
    "layer_module_tmp": "model.language_model.layers.{}",
    "mlp_module_tmp": "model.language_model.layers.{}.mlp",
    "attn_module_tmp": "model.language_model.layers.{}.self_attn",
    "ln_f_module": "model.language_model.norm",
    "lm_head_module": "model.language_model.embed_tokens",
    "mom2_dataset": "wikipedia",
    "mom2_n_samples": 2000,
    "mom2_dtype": "float32",
    "window_size": 10,
    "overlap": 5
}
